{
  "name": "@djpfs/react-vlibras-typescript",
  "description": "Lib do VLibras para usar em seus projetos React que usam typescript",
  "version": "1.0.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/djpfs/react-vlibras.git#typescript"
  },
  "keywords": [
    "vlibras",
    "react"
  ],
  "author": "@djpfs",
  "license": "ISC",
  "prepare": "npm run build",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  }
}
